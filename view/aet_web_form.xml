<?xml version="1.0"?>
<form>
    <notebook>
        <page id="Program Data" string="Program Data">
            <group string="Program"  colspan="4" col="6" id="program">
                <label name="name"/>
                <field name="name" readonly="1"/>
                <label name="gender"/>
                <field name="gender"/>
                <newline/>
                <label name="category" />
                <field name="category" widget='selection' colspan="3"/>
                <newline/>
                <label name="live"/>
                <field name="live"/>
                <label name="locality_of_emission"/>
                <field name="locality_of_emission"/>
                <newline/>
                <label name="date_of_emission"/>
                <field name="date_of_emission"/>
                <label name="duration"/>
                <field name="duration"/>
                <newline/>
                <label name="description"/>
                <field name="description"/>
                <label name="enrolled"/>
                <field name="enrolled"/>
            </group>
            <group id="buttons" colspan="1" col="2">
                <button name="sign_up" string="Sign up" icon="tryton-ok"/>
            </group>
        </page>
        <page id="Video" string="Video">
            <group string="Program"  colspan="4" col="6" id="program">
                <label name="video_long"/>
                <field name="video_long" readonly="1"/>
                <label name="video_short"/>
                <field name="video_short" colspan="3"/>
            </group>
        </page>
        <page id="Production Data" string="Production Data">
            <group string="Production Data"  colspan="4" col="6" id="Production Data">
                <label name="producer"/>
                <field name="producer" readonly="1"/>
                <label name="co_producer"/>
                <field name="co_producer" colspan="3"/>
                <label name="author"/>
                <field name="author" colspan="3"/>
                <newline/>
                <label name="editor"/>
                <field name="editor" colspan="3"/>
                <label name="director"/>
                <field name="director" colspan="3"/>
                <label name="cameraman"/>
                <field name="cameraman" colspan="3"/>
                <newline/>
                <label name="musician"/>
                <field name="musician" colspan="3"/>
                <label name="host"/>
                <field name="host" colspan="3"/>
                <label name="protagonist"/>
                <field name="protagonist" colspan="3"/>
                <newline/>
                <label name="chroniclers"/>
                <field name="chroniclers" colspan="3"/>
            </group>
        </page>
        <page id="Channel Data" string="Channel Data">
            <group string="Channel Data"  colspan="4" col="6" id="Channel Data">
                <label name="channel_name"/>
                <field name="channel_name" readonly="1"/>
                <label name="address_channel"/>
                <field name="address_channel" colspan="3"/>
                <label name="channel_town"/>
                <field name="channel_town" colspan="3"/>
                <newline/>
                <label name="channel_contact"/>
                <field name="channel_contact" colspan="3"/>
                <label name="channel_phone"/>
                <field name="channel_phone" colspan="3"/>
                <label name="channel_email"/>
                <field name="channel_email" colspan="3"/>
                <newline/>
                <label name="aet_partner"/>
                <field name="aet_partner" colspan="3"/>
            </group>
        </page>
        <page id="Billing Data" string="Billing Data">
            <group string="Billing Data"  colspan="4" col="6" id="Billing Data">
                <label name="business_name"/>
                <field name="business_name" readonly="1"/>
                <label name="cuit"/>
                <field name="cuit" colspan="3"/>
            </group>
        </page>
    </notebook>
</form>