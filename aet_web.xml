<?xml version="1.0"?>
<tryton>
    <data>
        <record model="ir.ui.view" id="aet_web_view_tree">
            <field name="model">aet_web.aet_web</field>
            <field name="type">tree</field>
            <field name="name">aet_web_tree</field>
        </record>
        <record model="ir.ui.view" id="aet_web_view_form">
            <field name="model">aet_web.aet_web</field>
            <field name="type">form</field>
            <field name="name">aet_web_form</field>
        </record>

        <record model="ir.action.act_window" id="act_aet_web_form_open">
            <field name="name">aet</field>
            <field name="res_model">aet_web.aet_web</field>
        </record>
        <record model="ir.action.act_window.view" id="act_aet_web_open_view1">
            <field name="sequence" eval="10"/>
            <field name="view" ref="aet_web_view_tree"/>
            <field name="act_window" ref="act_aet_web_form_open"/>
        </record>
        <record model="ir.action.act_window.view" id="act_aet_web_open_view2">
            <field name="sequence" eval="20"/>
            <field name="view" ref="aet_web_view_form"/>
            <field name="act_window" ref="act_aet_web_form_open"/>
        </record>
        <menuitem name="AET"
                id="aet_web_menu"/>

        <menuitem
            parent="aet_web_menu"
            action="act_aet_web_form_open"
            sequence="10"
            id="aet_web_menu_form"/>

        <record model="ir.ui.view" id="category_view_tree">
            <field name="model">aet_web.category</field>
            <field name="type">tree</field>
            <field name="name">aet_web_category_tree</field>
        </record>
        <record model="ir.ui.view" id="category_line_view_form">
            <field name="model">aet_web.category</field>
            <field name="type">form</field>
            <field name="name">aet_web_category_form</field>
        </record>

    </data>
</tryton>